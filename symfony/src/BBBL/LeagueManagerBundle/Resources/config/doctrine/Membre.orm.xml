<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BBBL\LeagueManagerBundle\Entity\Membre" table="membre">
    <id name="idmembre" type="integer" column="idmembre">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pseudo" type="string" column="pseudo" length="100" nullable="false"/>
    <field name="email" type="string" column="email" length="100" nullable="true"/>
    <field name="pass" type="string" column="pass" length="50" nullable="false"/>
    <field name="droits" type="integer" column="droits" nullable="true"/>
    <field name="hoverNav" type="boolean" column="hover_nav" nullable="true"/>
    <field name="hoverNavPlace" type="boolean" column="hover_nav_place" nullable="true"/>
    <field name="tresorerie" type="integer" column="tresorerie" nullable="true"/>
    <field name="policeDefaut" type="string" column="police_defaut" length="50" nullable="true"/>
    <field name="actif" type="boolean" column="actif" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
  </entity>
</doctrine-mapping>
