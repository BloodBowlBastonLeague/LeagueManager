            <tr>
                <th>Idteam</th>
                <td>{{ entity.idteam }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('team') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('team_edit', { 'id': entity.idteam }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
