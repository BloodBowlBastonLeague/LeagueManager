            </tr>
            <tr>
                <th>Tresor</th>
                <td>{{ entity.tresor }}</td>
            </tr>
            <tr>
                <th>Reroll</th>
                <td>{{ entity.reroll }}</td>
            </tr>
            <tr>
                <th>Popularite</th>
                <td>{{ entity.popularite }}</td>
            </tr>
            <tr>
                <th>Urlphoto</th>
                <td>{{ entity.urlPhoto }}</td>
