{% extends '::base.html.twig' %}

{% block body -%}
    <h1>SaisonLigue</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Saison</th>
                <td>{{ entity.saison }}</td>
            </tr>
            <tr>
                <th>Modeligue</th>
                <td>{{ entity.modeLigue }}</td>
            </tr>
            <tr>
                <th>Ordre</th>
                <td>{{ entity.ordre }}</td>
            </tr>
            <tr>
                <th>Datedeb</th>
                <td>{{ entity.dateDeb|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Datefin</th>
                <td>{{ entity.dateFin|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Actif</th>
                <td>{{ entity.actif }}</td>
            </tr>
            <tr>
                <th>Idsaisonligue</th>
                <td>{{ entity.idsaisonLigue }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('saisonligue') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('saisonligue_edit', { 'id': entity.idsaisonLigue }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
